{
  "name": "Roblox-Tag-Editor",
  "globIgnorePaths": ["**/*.spec.lua", "**/*.spec"],
  "servePort": 12345,
  "tree": {
    "$className": "DataModel",
    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },
    "ServerStorage": {
      "$className": "ServerStorage",
      "TagEditor": {
        "$path": "src"
      }
    }
  }
}